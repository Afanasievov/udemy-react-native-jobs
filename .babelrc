{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "components": "./src/components",
        "constants": "./src/constants",
        "screens": "./src/screens",
        "utils": "./src/utils"
      }
    }]
  ]
}
